                $.each(data, function (index, row) {
                    if (params.get('IDPX') == row.ID) {
                        $("#IDPX").append("<option value ='" + row.ID + "' selected>" + row.TenPX + "</option>")
                        $("#IDPX").change();
                        //$("#IDKhoi").change();
                    }
                    else {
                        $("#IDPX").append("<option value ='" + row.ID + "'>" + row.TenPX + "</option>")
                    }

