                            $("#IDNhom").append("<option value ='" + row.IDNhom + "'>" + row.TenNhom + "</option>")
                        }
                    });
                }
                else {
                    $("#IDNhom").empty();
                }
            });
            $.get("/FGroup/GetTo", { IDPB: idpb, IDKhoi: idk }, function (data1) {
